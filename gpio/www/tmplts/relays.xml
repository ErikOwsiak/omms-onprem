   <div id="gpioRoof" class="relay-box waveshare3chHat" bus="onboard">
      <div class="head">
         <p>Roof Relays</p>
      </div>
      <div class="channels">
         <div devid="gpioRoof" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="gpioRoof" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="gpioRoof" ch="3" class="gpio-channel">
