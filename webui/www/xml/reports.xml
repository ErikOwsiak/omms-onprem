<div id="xmlReports">
   
   <div id="reportsMenuHead" class="reports-menu-head">
      <h1>reports</h1>
      <p>create &amp; run reports</p>
   </div>
   
   <div id="reportsMenuBody" class="reports-menu-body">
      <fieldset class="settings-btns">
         <legend>kWh Reports</legend>
         <navbtn id="kWhrsReports" class="nav-btn">
            <div class="nav-btn-lbl">
               <sapn>Show Reports</sapn>
            </div>
            <div class="nav-sub-btns">
               <div></div>
            </div>
         </navbtn>
         <navbtn id="monthPicker" class="nav-btn mn-picker">
            <div class="nav-btn-lbl" style="text-align: center;">
               <select id="yearSel">
                  <option value="">-- year --</option>
               </select>
               <select id="monthSel">
                  <option value="">-- month --</option>
                  <option value="1">January</option>
                  <option value="2">February</option>
                  <option value="3">March</option>
                  <option value="4">April</option>
                  <option value="5">May</option>
                  <option value="6">June</option>
                  <option value="7">July</option>
                  <option value="8">August</option>
                  <option value="9">September</option>
                  <option value="10">October</option>
                  <option value="11">November</option>
                  <option value="12">December</option>
               </select>
               <input type="button" id="btnRunRpt" value="Run"/>
            </div>
            <div class="">
               <div></div>
            </div>
         </navbtn>
         <div id="rptFeedbackDiv" class="rpt-feedback-div">
         </div>
      </fieldset>
   </div>
   __script
      _omms.app.subNav = new submenuNav("reportsMenuBody");
      _omms.xmlsecs.initReports();
      _omms.xmlsecs.LOADED_XML = "xmlReports";
      console.log(`##script: ${_omms.xmlsecs.LOADED_XML} :script##`);
   script__
</div>
