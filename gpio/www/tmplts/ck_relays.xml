<div class="relay-col">
   <div class="clt-head">
      VersusCK : SiteGPIO
   </div>
   <!-- roof relays -->
   <div id="gpioRoof" type="waveshare3chHat" class="relay-box waveshare3chHat" bus="onboard">
      <div class="head">
         <div>
            <p>CK Roof</p>
            <dtype>waveshare3chHat</dtype>
         </div>
      </div>
      <div class="channels">
         <div devid="gpioRoof" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="gpioRoof" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="gpioRoof" ch="3" class="gpio-channel">
            <div class="ch-num">CH3</div>
         </div>
         <div devid="gpioRoof" ch="4" class="gpio-channel disable">
            <div class="ch-num">CH4</div>
         </div>
      </div>
      <div id="gpioRoofConf" class="channel-conf">
      </div>
   </div>
   <!-- p16 0 relays -->
   <div id="gpioP16_0" type="lctech4chModbus" class="relay-box lctech4chModbus" bus="modbus">
      <div class="head">
         <div>
            <p>CK B0.T (a)</p>
            <dtype>lctech4chModbus</dtype>
         </div>
      </div>
      <div class="channels">
         <div devid="gpioP16_0" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="gpioP16_0" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="gpioP16_0" ch="3" class="gpio-channel">
            <div class="ch-num">CH3</div>
         </div>
         <div devid="gpioP16_0" ch="4" class="gpio-channel">
            <div class="ch-num">CH4</div>
         </div>
      </div>
      <div id="gpioP16_0Conf" class="channel-conf">
      </div>
   </div>
   <!-- p16 1 relays -->
   <div id="gpioP16_1" type="lctech4chModbus" class="relay-box lctech4chModbus" bus="modbus">
      <div class="head">
         <div>
            <p>CK B0.T (b)</p>
            <dtype>lctech4chModbus</dtype>
         </div>
      </div>
      <div class="channels">
         <div devid="gpioP16_1" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="gpioP16_1" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="gpioP16_1" ch="3" class="gpio-channel">
            <div class="ch-num">CH3</div>
         </div>
         <div devid="gpioP16_1" ch="4" class="gpio-channel">
            <div class="ch-num">CH4</div>
         </div>
      </div>
      <div id="gpioP16_1Conf" class="channel-conf">
      </div>
   </div>
   <!-- p7 0 relays -->
   <div id="gpioP7_0" type="lctech4chModbus" class="relay-box lctech4chModbus" bus="modbus">
      <div class="head">
         <div>
            <p>CK A0.T (a)</p>
            <dtype>lctech4chModbus</dtype>
         </div>
      </div>
      <div class="channels">
         <div devid="gpioP7_0" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="gpioP7_0" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="gpioP7_0" ch="3" class="gpio-channel">
            <div class="ch-num">CH3</div>
         </div>
         <div devid="gpioP7_0" ch="4" class="gpio-channel">
            <div class="ch-num">CH4</div>
         </div>
      </div>
      <div id="gpioP7_0Conf" class="channel-conf">
      </div>
   </div>

</div>
