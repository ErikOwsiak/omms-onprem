<div class="relay-col">
   <div class="clt-head">
      VersusCK : SiteGPIO
   </div>
   <!-- roof relays -->
   <div id="roof0" class="relay-box waveshare3chHat" bus="onboard">
      <div class="head">
         <p>CK Roof Relays</p>
      </div>
      <div class="channels">
         <div devid="roof0" ch="1" class="gpio-channel">
            <div class="ch-num">CH1</div>
         </div>
         <div devid="roof0" ch="2" class="gpio-channel">
            <div class="ch-num">CH2</div>
         </div>
         <div devid="roof0" ch="3" class="gpio-channel">
            <div class="ch-num">CH3</div>
         </div>
         <div devid="roof0" ch="4" class="gpio-channel disable">
            <div class="ch-num">CH4</div>
         </div>
      </div>
      <div id="roof0Conf" class="channel-conf">
      </div>
   </div>
   <!-- p16 0 relays -->
